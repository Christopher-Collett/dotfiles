[global_config]
  inactive_color_offset = 1.0
  smart_copy = False
  clear_select_on_copy = True
[keybindings]
[profiles]
  [[default]]
    background_darkness = 0.8
    background_type = image
    background_image = HOME_DIR/.config/terminator/background.jpg
    background_image_mode = scale_and_crop
    cursor_fg_color = "#000000"
    cursor_bg_color = "#aaaaaa"
    font = Roboto Mono Light for Powerline Bold 11
    show_titlebar = False
    scrollbar_position = hidden
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
